{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
      "config:base",
      ":enablePreCommit",
      "group:allNonMajor"
  ],
  "dockerfile": {
    "fileMatch": [
      "(^|/|\\.)Containerfile$",
      "(^|/)Containerfile[^/]*$"
    ]
  },
  "automerge": false,
  "schedule": [
      "every weekend"
  ]
}
