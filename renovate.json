{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>platform-engineering-org/.github"
  ],
  "rangeStrategy": "bump",
  "automerge": false,
  "packageRules": [
    {
      "matchPackageNames": [
        "go",
        "registry.access.redhat.com/ubi9/go-toolset"
      ],
      "dependencyDashboardApproval": true,
      "groupName": "go upgrade"
    }
  ]
}
